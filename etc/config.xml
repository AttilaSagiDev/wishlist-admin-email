<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <wishlist_admin_email>
            <wishlist_admin_email_config>
                <enabled>0</enabled>
                <enable_segmentation>0</enable_segmentation>
            </wishlist_admin_email_config>
            <wishlist_admin_email_email>
                <recipient_email><![CDATA[hello@example.com]]></recipient_email>
                <sender_email_identity>custom2</sender_email_identity>
                <email_template>wishlist_admin_email_wishlist_admin_email_email_email_template</email_template>
            </wishlist_admin_email_email>
        </wishlist_admin_email>
    </default>
</config>
